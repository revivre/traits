<h1>WorkTimes#show</h1>
<table class="table table-bordered">
  <tr>
      <th>id</th>
      <th>user_id</th>
      <th>start_time</th>
      <th>end_time</th>
  </tr>
  <tr>
      <td><%= link_to @work_time.id, {:controller => "work_times", :action => "edit", :id => @work_time.id} %></td>
      <td><%= @work_time.user_id %></td>
      <td><%= format_date @work_time.start_time %></td>
      <td><%= format_date @work_time.end_time %></td>
  </tr>
</table>
